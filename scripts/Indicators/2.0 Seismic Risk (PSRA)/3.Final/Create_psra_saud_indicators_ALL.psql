CAST(CAST(ROUND(CAST(SUM(COALESCE((((CASE WHEN a.genocc ='Residential-LD' THEN a.night/a.popdu ELSE 0 END) * 
((0 * (CASE WHEN a.genocc ='Residential-LD' THEN e.structural_moderate_mean  / a.number ELSE 0 END)) + 
